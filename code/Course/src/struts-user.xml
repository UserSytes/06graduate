	<package name="teacourse" extends="struts-default">
		<interceptors>  
        <!-- 定义拦截器栈 -->
			<interceptor-stack name="fileTextStack">
				<interceptor-ref name="fileUpload">
					<param name="maximumSize">104857600</param> 
					<param name="allowedTypes">
						application/msword,application/vnd.ms-powerpoint,application/pdfW,text/plain</param>
				</interceptor-ref>
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>
			<interceptor-stack name="fileExcelStack">
				<interceptor-ref name="fileUpload">
					<param name="maximumSize">10485760</param> 
					<param name="allowedTypes">
						application/excel,application/vnd.ms-excel</param>
				</interceptor-ref>
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>
			<interceptor-stack name="fileImgStack">
				<interceptor-ref name="fileUpload">
					<param name="maximumSize">1048576</param> 
					<param name="allowedTypes">
						image/x-ms-bmp,image/jpeg,image/gif,image/png,image/pjpeg,image/x-png</param>
				</interceptor-ref>
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>
			<interceptor-stack name="fileMovieStack">
				<interceptor-ref name="fileUpload">
				<param name="maximumSize">104857600</param> 
					<param name="allowedTypes">
						video/x-msvideo,video/x-sgi-movie,video/mpeg,application/x-pn-realaudio,audio/x-wav</param>
				</interceptor-ref>
				<interceptor-ref name="defaultStack" />
			</interceptor-stack>
		</interceptors>
		</package>
		
